<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar fx:id="menuBar" layoutY="2.0" prefWidth="644.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uireturns.controllers.MenuBarController">
    <menus>
        <Menu fx:id="file" mnemonicParsing="false" text="File">
            <items>
                <MenuItem fx:id="fileclose" mnemonicParsing="false" text="Close" />
            </items>
        </Menu>
        <Menu fx:id="edit" mnemonicParsing="false" text="Edit">
            <items>
                <MenuItem fx:id="delete" mnemonicParsing="false" text="Delete" />
            </items>
        </Menu>
        <Menu fx:id="about" mnemonicParsing="false" text="About">
         <items>
            <MenuItem fx:id="aboutParser" mnemonicParsing="false" onAction="#aboutParser" text="About" />
         </items></Menu>
    </menus>
</MenuBar>
